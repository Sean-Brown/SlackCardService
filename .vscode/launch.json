{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Server",
            "program": "${workspaceFolder}\\app.js",
            "preLaunchTask": "tsc",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "NODE_ENV": "development",
                "DB_HOST": "localhost",
                "DB_PORT": "5432",
                "DB_NAME": "slackcardservice",
                "DB_USER": "postgres",
                "DB_PASS": "pghello5",
                "DB_DIALECT": "postgres",
                "ST_JOIN_GAME": "abc123",
                "ST_DESCRIBE": "abc123",
                "ST_RESET_GAME": "abc123",
                "ST_BEGIN_GAME": "abc123",
                "ST_SHOW_HAND": "abc123",
                "ST_PLAY_CARD": "abc123",
                "ST_THROW_CARD": "abc123",
                "ST_GO": "abc123",
                "ST_UNFINISHED_GAMES": "abc123",
                "ST_LEAVE_GAME": "abc123",
                "ST_CURRENT_GAME": "abc123",
                "CRIB_RESET_SECRET": "super-secret",
                "AWS_S3_STANDARD_DECK_URL": "https://s3.amazonaws.com/slackcardservice/StandardDeck/"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test Spec",
            "program": "${workspaceFolder}\\node_modules\\jasmine-node\\bin\\jasmine-node",
            "args": [
                "./spec/SlackCardService/"
            ],
            "preLaunchTask": "tsc",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test Node",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceFolder}\\node_modules\\mocha\\bin\\_mocha",
            "args": [
                "-c",
                "-b",
                "--reporter",
                "spec",
                "--exit",
                "spec/Node/*.js"
            ],
            "runtimeArgs": [
                "--nolazy"
            ],
            "preLaunchTask": "tsc",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "outputCapture": "std",
            "sourceMaps": true
        }
    ]
}